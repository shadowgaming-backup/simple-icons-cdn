{
  "version": 2,
  "rewrites": [
    {
      "source": "/:iconSlug/:color?/:darkModeColor?",
      "destination": "/api"
    }
  ],
  "trailingSlash": false,
  "installCommand": "npm i --omit=dev && npm update simple-icons",
  "ignoreCommand": "if [ \"$VERCEL_ENV\" == \"production\" ]; then exit 1; else exit 0; fi"
}
